{
  "name": "airbuddy-security",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@4.9.1",
  "module": "./dist/index.js",
  "scripts": {
    "dev": "tsx watch ./src/index.ts",
