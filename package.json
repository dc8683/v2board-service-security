    "@koa/router": "^13.1.0",
    "@types/bun": "latest",
    "@types/koa": "^2.15.0",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^20.0",
    "eslint": "^9.15.0",
    "eslint-plugin-format": "^0.1.2",
    "husky": "^9.1.7",
    "is-ci": "^3.0.1",
    "koa": "^3.0.0",
    "lint-staged": "^15.2.0",
    "tsx": "^4.19.4",
    "typescript": "^5.7.2"
  }
